{
	"compilerOptions": {
		"outDir": "./build/",
		"target": "ESNext",
		"jsx": "react-jsx",
		"module": "commonjs",
		"noImplicitAny": true,
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": [ "src" ]
}
